#include <vector>
#include <cstdio>
#include <ctime>
#include <cassert>
#include <algorithm>
#include <iostream>
using namespace std;


void mergeFunc(vector<int> &v, int l, int m, int r, vector<int> &tmp){
	int pos1 = l;
	int pos2 = m + 1;
	int posBuff = l;
	while(pos1 <= m && pos2 <= r){
		cout << "comparing " << v[pos1] <<" and " << v[pos2] << endl;
		if(v[pos1]<v[pos2]) tmp[posBuff++] = v[pos1++];
		else tmp[posBuff++] = v[pos2++];
	}
	while(pos2 <= r)
		tmp[posBuff++] = v[pos2++];
	while(pos1 <= m)
		tmp[posBuff++] = v[pos1++];
	for (int i = 0; i < r - l + 1; i++)
		v[l+i] = tmp[l+i];
}
void mergeSort(vector<int> &v, int l, int r, vector<int> &tmp){
	cout << "sorting segment [" << l + 1 << ", " << r + 1 <<"]" << endl;
	if(l < r){
		int m = (l + r) / 2;
		if(m + 1 > 2) mergeSort(v, l, m, tmp);
		if(r + 1 > 2) mergeSort(v, m + 1, r, tmp);
		mergeFunc(v, l, m, r, tmp);
	}
	cout <<  "segment [" << l + 1 << ", " << r + 1 << "] sorted" << endl;
}
void mergeSort(vector<int> &v){
	if(!v.empty()){
		vector<int> tmp(v.size());
		mergeSort(v, 0, v.size() - 1, tmp);
	}
}

int main(){
	int n;
	cin >> n;
	vector<int> a(n);
	for(int i = 0; i < n; i++)
		cin >> a[i];
	mergeSort(a);
	cout << "result:";
	for(int i = 0; i < n; i++)
		cout << " " << a[i];

	system("pause");
	return 0;
}
